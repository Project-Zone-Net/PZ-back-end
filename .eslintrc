{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
  "@typescript-eslint"
  ],
  "extends": [
  "eslint:recommended",
  "plugin:@typescript-eslint/eslint-recommended",
  "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-var-requires": 0,
    "no-console": 1,
    "@typescript-eslint/quotes": ["error", "single"],
    "quotes": ["warn", "single"],
    "semi": ["error", "never"]
  }
}